<p id="notice"><%= notice %></p>

<h1>Application Forms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>About</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Project title</th>
      <th>Project description</th>
      <th>Age</th>
      <th>Post</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @application_forms.each do |application_form| %>
      <tr>
        <td><%= application_form.name %></td>
        <td><%= application_form.about %></td>
        <td><%= application_form.email %></td>
        <td><%= application_form.phone %></td>
        <td><%= application_form.project_title %></td>
        <td><%= application_form.project_description %></td>
        <td><%= application_form.age %></td>
        <td><%= application_form.post_id %></td>
        <td><%= link_to 'Show', post_application_form_path(@post, application_form) %></td>
        <td><%= link_to 'Edit', edit_post_application_form_path(@post, application_form) %></td>
        <td><%#= link_to 'Destroy', application_form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%#= link_to 'New Application Form', new_application_form_path %>
