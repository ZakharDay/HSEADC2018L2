<p id="notice"><%= notice %></p>

<h1>Application Settings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>About</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Project title</th>
      <th>Project description</th>
      <th>Age</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @application_settings.each do |application_setting| %>
      <tr>
        <td><%= application_setting.name %></td>
        <td><%= application_setting.about %></td>
        <td><%= application_setting.email %></td>
        <td><%= application_setting.phone %></td>
        <td><%= application_setting.project_title %></td>
        <td><%= application_setting.project_description %></td>
        <td><%= application_setting.age %></td>
        <td><%= link_to 'Show', application_setting %></td>
        <td><%= link_to 'Edit', edit_application_setting_path(application_setting) %></td>
        <td><%= link_to 'Destroy', application_setting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

